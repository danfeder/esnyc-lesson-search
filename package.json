{
  "name": "esnyc-lesson-search",
  "version": "1.0.0",
  "description": "ESNYC Lesson Library Search Interface - A searchable database of 831 tagged Edible Schoolyard NYC lesson plans",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "serve": "python3 -m http.server 8000",
    "serve:npm": "npx http-server -p 8000",
    "dev": "npx live-server --port=8000"
  },
  "keywords": [
    "education",
    "lesson-plans",
    "search",
    "edible-schoolyard",
    "teaching-resources"
  ],
  "author": "ESNYC",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "live-server": "^1.2.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}